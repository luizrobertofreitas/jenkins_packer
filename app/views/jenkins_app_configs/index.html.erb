<h1>Listing jenkins_app_configs</h1>

<table>
  <thead>
    <tr>
      <th>Directory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jenkins_app_configs.each do |jenkins_app_config| %>
      <tr>
        <td><%= jenkins_app_config.directory %></td>
        <td><%= link_to 'Show', jenkins_app_config %></td>
        <td><%= link_to 'Edit', edit_jenkins_app_config_path(jenkins_app_config) %></td>
        <td><%= link_to 'Destroy', jenkins_app_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jenkins app config', new_jenkins_app_config_path %>
